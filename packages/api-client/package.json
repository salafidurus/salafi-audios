{
  "name": "@sd/api-client",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm codegen && tsup src/index.ts --format esm,cjs --dts --out-dir dist",
    "dev": "pnpm build --watch",
    "codegen": "orval --config ./orval.config.cjs && node ./scripts/postprocess-generated.cjs",
    "lint": "eslint .",
    "test": "echo \"@sd/api-client: skipped (generated)\"",
    "test:prepush": "echo \"@sd/api-client: skipped (generated)\"",
    "typecheck": "pnpm codegen && tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@sd/config": "workspace:*",
    "orval": "^7.10.0",
    "rimraf": "^6.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.0"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5",
    "react": ">=18"
  }
}
